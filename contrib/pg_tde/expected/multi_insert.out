CREATE TABLE albums (
    album_id INTEGER GENERATED ALWAYS AS IDENTITY PRIMARY KEY,
    artist_id INTEGER,
    title TEXT NOT NULL,
    released DATE NOT NULL
) USING :tde_am;
COPY albums FROM stdin CSV HEADER;
SELECT * FROM albums;
